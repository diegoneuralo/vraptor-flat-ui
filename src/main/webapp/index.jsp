<!DOCTYPE HTML>
<html lang="en">
	<head>
		<meta charset="utf-8">
	    <title>VRaptor with Flat UI</title>
	    <meta name="viewport" content="width=device-width, initial-scale=1.0">
	
	    <!-- Loading Bootstrap -->
	    <link href="resources/css/bootstrap.css" rel="stylesheet">
	
	    <!-- Loading Flat UI -->
	    <link href="resources/css/style.css" rel="stylesheet">
	    <link rel="shortcut icon" href="resources/images/favicon.ico">
	
	    <!-- HTML5 shim, for IE6-8 support of HTML5 elements. All other JS at the end of file. -->
	    <!--[if lt IE 9]>
	      <script src="resources/js/html5shiv.js"></script>
	    <![endif]-->
	</head>
	<body>
		<section class="login">
	    	<div class="login-screen">
		        <div class="login-icon">
			          <img src="resources/images/login/icon.png" alt="Welcome to Mail App" />
			          <h4>Welcome to <small>your App</small></h4>
		        </div>
	
		        <div class="login-form">
			          <div class="control-group">
				          <input type="text" class="login-field" value="" placeholder="Enter your name" id="login-name" />
				          <label class="login-field-icon fui-man-16" for="login-name"></label>
			          </div>
		
			          <div class="control-group">
				          <input type="password" class="login-field" value="" placeholder="Password" id="login-pass" />
				          <label class="login-field-icon fui-lock-16" for="login-pass"></label>
			          </div>
			          <a class="btn btn-primary btn-large btn-block" href="home.jsp">Login</a>
			          <a class="login-link" href="#">Lost your password?</a>
		        </div>
	    	</div>
		</section>
		<!-- Load JS here for greater good =============================-->
	    <script src="resources/js/jquery-1.8.2.min.js"></script>
	    <script src="resources/js/jquery-ui-1.10.0.custom.min.js"></script>
	    <script src="resources/js/jquery.dropkick-1.0.0.js"></script>
	    <script src="resources/js/custom_checkbox_and_radio.js"></script>
	    <script src="resources/js/custom_radio.js"></script>
	    <script src="resources/js/jquery.tagsinput.js"></script>
	    <script src="resources/js/bootstrap-tooltip.js"></script>
	    <script src="resources/js/jquery.placeholder.js"></script>
	    <script src="http://vjs.zencdn.net/c/video.js"></script>
	    <script src="resources/js/application.js"></script>
	    <!--[if lt IE 8]>
	      <script src="resources/js/icon-font-ie7.js"></script>
	      <script src="resources/js/icon-font-ie7-24.js"></script>
	    <![endif]-->
	</body>
</html>